<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 2.8.0//EN"
        "http://gwtproject.org/doctype/2.8.0/gwt-module.dtd">
<module rename-to="MySampleApplication">

    <!-- Inherit the core Web Toolkit stuff.                  -->
    <inherits name='com.google.gwt.user.User'/>
    <inherits name="com.app.App"/>
    <inherits name="com.google.gwt.i18n.I18N"/>
    <inherits name="com.google.gwt.user.UI"/>
    <inherits name="com.google.gwt.uibinder.UiBinder"/>

    <!-- Specify the app entry point class.                   -->
    <entry-point class='com.app.client.App'/>



    <!-- Specify the app servlets.                   -->
    <servlet path='/MySampleApplicationService' class='com.app.server.impl.AppImpl'/>
    <!-- Specify the paths for translatable code -->
    <source path='client'/>
    <source path='shared'/>

</module>